				"x": {"type":"number"},
				"y": {"type":"number"},
				"z": {"type":"number"}
			},
			"required": ["x", "y", "z"]
		},
		"y": {
			"title": "Coordinate",
			"type": "object",
			"default": {
				"x": 0,
				"y": 1,
				"z": 0
			},
			"properties": {
				"x": {"type":"number"},
				"y": {"type":"number"},
				"z": {"type":"number"}
			},
			"required": ["x", "y", "z"]
		},
		"z": {
			"title": "Coordinate",
			"type": "object",
			"default": {
				"x": 0,
				"y": 0,
				"z": 1
			},
			"properties": {
				"x": {"type":"number"},
				"y": {"type":"number"},
				"z": {"type":"number"}
			},
			"required": ["x", "y", "z"]
		}
